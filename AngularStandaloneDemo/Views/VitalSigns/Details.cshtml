    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Temperature)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Temperature)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.BloodPressureSystolic)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BloodPressureSystolic)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.BloodPressureDiastolic)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.BloodPressureDiastolic)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.HeartRate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.HeartRate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.RespiratoryRate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RespiratoryRate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.OxygenSaturation)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.OxygenSaturation)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Weight)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Weight)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Remarks)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Remarks)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.RecordedAt)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RecordedAt)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Patient)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Patient.Id)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
